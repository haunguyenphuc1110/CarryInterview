{
  "compilerOptions": {
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "isolatedModules": true,
    "resolveJsonModule": true,
    "jsx": "react",
    "lib": [
      "es6",
      "es2015",
      "dom"
    ],
    "moduleResolution": "node",
    "noEmit": true,
    "strict": true,
    "target": "esnext",
    "baseUrl": ".",
    "paths": {
      "*": [
        "src/*"
      ],
      "Components/*": [
        "src/components/*"
      ],
      "Constants/*": [
        "src/constants/*"
      ],
      "Navigators/*": [
        "src/navigators/*"
      ],
      "Features/*": [
        "src/features/*"
      ],
      "Services/*": [
        "src/services/*"
      ],
      "Models/*": [
        "src/models/*"
      ],
      "Hooks/*": [
        "src/hooks/*"
      ],
      "I18n/*": [
        "src/i18n/*"
      ],
      "Utils/*": [
        "src/utils/*"
      ],
      "Styles/*": [
        "src/styles/*"
      ]
    },
  },
  "exclude": [
    "node_modules",
    "babel.config.js",
    "metro.config.js",
    "jest.config.js"
  ]
}